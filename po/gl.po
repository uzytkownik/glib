msgstr "O URI '%s' non é un URI absoluto usando o esquema \"file\""

#: ../glib/gconvert.c:1743
#, c-format
msgid "The local file URI '%s' may not include a '#'"
msgstr "O URI do ficheiro local '%s' non pode incluír un '#'"

#: ../glib/gconvert.c:1760
#, c-format
